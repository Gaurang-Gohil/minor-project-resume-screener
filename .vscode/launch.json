{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: Debug with Uvicorn",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
  "backend.app:app",
  "--reload",
  "--host", "127.0.0.1",
  "--port", "8000"
],
      "jinja": true,
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
